{% extends 'base.html.twig' %}

{% block body %}
    <h3>
        All users
    </h3>
    <ul>
        {% for user in users %}
            <li class="list-unstyled">
                <a href="">
                    {{ user.username ~ (user.activeNow ? ' (online)' : '')}}
                </a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}


